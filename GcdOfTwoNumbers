
#include <iostream>
using namespace std;
int main() {
    
    int a,b,gcd=1;
    
    cout<<"Enter the value of a and b: ";
    cin>>a>>b;
    
    while(gcd){
        
        if(a>b){
            gcd = a - b;
            a = gcd;
        }
        else
        if(a<b){
            gcd = b - a;
            b = gcd;
        }
        else
            gcd  = a - b;
    
    }
    
    cout<<"Gcd of a and b: "<<a;

    return 0;
}
