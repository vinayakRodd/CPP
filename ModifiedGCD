#include <iostream>
using namespace std;
int main() {
    int a,b;
    
    cout<<"Enter the value of a and b: ";
    cin>>a>>b;
    
    while(a > 0 && b > 0){

        if(a>b)
            a = a - b;
            
        else
            b = b - a;
        
    
    }
    
    if(b==0)
        cout<<"Gcd of a and b: "<<a;
    else
        cout<<"Gcd of a and b: "<<b;

    return 0;
}
